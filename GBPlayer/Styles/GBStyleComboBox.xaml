<ResourceDictionary x:Uid="ResourceDictionary_1" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    mc:Ignorable="d">

  <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary x:Uid="ResourceDictionary_2" Source="../Styles/GBColors.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <!--TEMPLATE PAR DEFAUT POUR LE BOUTON DU COMBOBOX-->
    <ControlTemplate x:Uid="ControlTemplate_1" x:Key="ComboBoxToggleButton" TargetType="{x:Type ToggleButton}">
        <Grid x:Uid="Grid_1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition x:Uid="ColumnDefinition_1" />
                <ColumnDefinition x:Uid="ColumnDefinition_2" Width="20" />
            </Grid.ColumnDefinitions>
             <Border x:Uid="Border" x:Name="Border"
              Grid.ColumnSpan="2"
              BorderThickness="{TemplateBinding BorderThickness}"
              CornerRadius="5"
              Margin="0" 
              Background="{TemplateBinding Background}"
              BorderBrush="{TemplateBinding BorderBrush}"
              >
             <!--   <Border.Effect>
                    <DropShadowEffect  Direction="240" ShadowDepth="1" Color="{StaticResource APCouleurOmbreBackground}" BlurRadius="2" RenderingBias="Quality" />
                </Border.Effect> -->
            </Border>
            <Path x:Uid="Arrow" x:Name="Arrow"
                Grid.Column="1"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Margin="1,1,1,1"
                Fill="{TemplateBinding Foreground}"
                Data="M 4 0 L 8 4 L 4 8 Z" >
                <!--
                StrokeThickness="1"
               Data="M 0 0 L 4 4 L 0 8" Stroke="{TemplateBinding Foreground}" >
                <Path.Effect>
                    <DropShadowEffect Direction="90" ShadowDepth="1" Color="{StaticResource APCouleurOmbreForeground}" BlurRadius="3"/>
                </Path.Effect>-->
            </Path>
            <ContentPresenter x:Uid="ContentPresenter_1"/>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Uid="CommonStates" x:Name="CommonStates">
                    <VisualState x:Uid="Normal" x:Name="Normal" >
                    </VisualState>
                    <VisualState x:Uid="MouseOver" x:Name="MouseOver">
                        <Storyboard x:Uid="Storyboard_1">
                            <ObjectAnimationUsingKeyFrames x:Uid="ObjectAnimationUsingKeyFrames_1" Storyboard.TargetName="Border"
                                         Storyboard.TargetProperty="Background">
                                <ObjectAnimationUsingKeyFrames.KeyFrames>
                                     <DiscreteObjectKeyFrame x:Uid="DiscreteObjectKeyFrame_1" KeyTime="0:0:0"  Value="{StaticResource W8BackgroundOver}"/>
                                </ObjectAnimationUsingKeyFrames.KeyFrames>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames x:Uid="ObjectAnimationUsingKeyFrames_2" Storyboard.TargetName="Border"
                                         Storyboard.TargetProperty="BorderBrush">
                                <ObjectAnimationUsingKeyFrames.KeyFrames>
                                    <DiscreteObjectKeyFrame x:Uid="DiscreteObjectKeyFrame_2" KeyTime="0:0:0"  Value="{StaticResource W8BorderOver}"/>
                                </ObjectAnimationUsingKeyFrames.KeyFrames>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard> 
                      <!--  <Storyboard >
                            <ColorAnimation Storyboard.TargetName="BorderColor" Storyboard.TargetProperty="Color"
                                                       To="{StaticResource W8BorderOverColor}" Duration="0:0:0.4"/>
                            <ColorAnimation Storyboard.TargetName="BackgroundColor" Storyboard.TargetProperty="Color"
                                                       To="{StaticResource W8BackgroundOverColor}" Duration="0:0:0.4"/>
                            <ThicknessAnimationUsingKeyFrames Storyboard.TargetName="Border"
                                                                          Storyboard.TargetProperty="BorderThickness">
                                <EasingThicknessKeyFrame KeyTime="0:0:0.4" Value="2"/>
                            </ThicknessAnimationUsingKeyFrames>
                        </Storyboard>-->
                    </VisualState>

                </VisualStateGroup>
                <VisualStateGroup x:Uid="CheckStates" x:Name="CheckStates">
                    <VisualState x:Uid="Pressed" x:Name="Pressed" />
                    <VisualState x:Uid="Disabled" x:Name="Disabled"/>
                    <VisualState x:Uid="Unchecked" x:Name="Unchecked" />
                    <VisualState x:Uid="Indeterminate" x:Name="Indeterminate" />
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
        <ControlTemplate.Triggers>
            <!--TRIGGER EXECUTE SI ELEMENT COCHE-->
            <Trigger x:Uid="Trigger_1" Property="IsChecked" Value="True">
                <Setter x:Uid="Setter_1" Property="Data"
                                TargetName="Arrow"
                                Value="M 0,0 L 8,0 L 4,4 Z"/>
                <!--"M 8 0 L 4 4 L 0 0"/>
                <Setter TargetName="Arrow"
                                Property="Stroke"
                                Value="{StaticResource W8BorderOver}"/>-->
                <Setter x:Uid="Setter_2" TargetName="Arrow"
                                Property="Fill"
                                Value="{StaticResource W8BorderOver}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <!--STYLE PAR DEFAUT POUR LES COMBOBOX-->
    <Style x:Uid="Style_1" x:Key="{x:Type ComboBox}" TargetType="{x:Type ComboBox}">
    <Setter x:Uid="Setter_3" Property="SnapsToDevicePixels" Value="true" />
    <Setter x:Uid="Setter_4" Property="OverridesDefaultStyle" Value="true" />
    <Setter x:Uid="Setter_5" Property="ScrollViewer.HorizontalScrollBarVisibility"  Value="Auto" />
    <Setter x:Uid="Setter_6" Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto" />
    <Setter x:Uid="Setter_7" Property="ScrollViewer.CanContentScroll"  Value="true" />
    <Setter x:Uid="Setter_8" Property="MinWidth" Value="70" />
    <Setter x:Uid="Setter_9" Property="MinHeight" Value="20" />
    <Setter x:Uid="Setter_10" Property="Template">
      <Setter.Value>
        <ControlTemplate x:Uid="ControlTemplate_2" TargetType="{x:Type ComboBox}">
          <Grid x:Uid="Grid_2">
                       <ToggleButton x:Uid="ToggleButton" x:Name="ToggleButton"
                          Background="{TemplateBinding Background}"           
                          Template="{StaticResource ComboBoxToggleButton}"
                          Foreground="{TemplateBinding Foreground}"
                          BorderBrush="{TemplateBinding BorderBrush}"
                          Focusable="false"
                          ClickMode="Press"
                          IsChecked="{Binding IsDropDownOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}">
                            <Grid x:Uid="Grille" x:Name="Grille">
                                <Label x:Uid="ContentSite" Margin="1" Height="Auto" Width="Auto" Focusable="False" x:Name="ContentSite" 
                                       FontFamily="{TemplateBinding FontFamily}" 
                                       Foreground="{TemplateBinding Foreground}" 
                                       Background="Transparent"
                                       FontSize="{TemplateBinding FontSize}" 
                                       VerticalAlignment="Center" 
                                       HorizontalAlignment="Left" 
                                       Visibility="Visible" 
                                       IsHitTestVisible="False">
                                    <Label.Content>
                                        <ContentPresenter 
                                           x:Uid="ContentPresenter_2" Content="{TemplateBinding SelectionBoxItem}"
                                            ContentTemplate="{TemplateBinding SelectionBoxItemTemplate}"
                                            ContentTemplateSelector="{TemplateBinding ItemTemplateSelector}">
                                        </ContentPresenter>
                                    </Label.Content>
                                </Label>
                                <TextBox x:Uid="PART_EditableTextBox" Margin="1" x:Name="PART_EditableTextBox"
                                       FontFamily="{TemplateBinding FontFamily}" 
                                       Foreground="{TemplateBinding Foreground}" 
                                       SelectionBrush="{TemplateBinding Foreground}"
                                       CaretBrush="{TemplateBinding Foreground}"
                                       FontSize="{TemplateBinding FontSize}" 
                                         Focusable="true"
                                       Background="Transparent"
                                        BorderThickness="0"
                                        VerticalAlignment="Center" 
                                        HorizontalAlignment="Left" 
                                        Visibility="Hidden">
                                </TextBox>
                            </Grid>
                        </ToggleButton>
                        <Popup x:Uid="Popup" x:Name="Popup" 
                               Placement="Bottom"
                                IsOpen="{TemplateBinding IsDropDownOpen}"
                                AllowsTransparency="True"
                                Focusable="False"
                                PopupAnimation="Slide">
                            <Grid x:Uid="DropDown" x:Name="DropDown"
                                  Background="Transparent" 
                                    SnapsToDevicePixels="True"
                                    MinWidth="{TemplateBinding ActualWidth}"
                                    MaxHeight="{TemplateBinding MaxDropDownHeight}">
                                <Border x:Uid="DropDownBorder" x:Name="DropDownBorder" BorderThickness="1" Margin="0,2,0,0" CornerRadius="4"
                                        Background="{StaticResource W8Border}"
                                        BorderBrush="{StaticResource W8BorderGray}">
                                    <!--"{TemplateBinding BorderBrush}" >-->
                                    <ScrollViewer x:Uid="ScrollViewer_1" Background="Transparent" Style="{StaticResource GBStyleScrollViewer}" Margin="4,6,4,6" 
                                              SnapsToDevicePixels="True" KeyboardNavigation.DirectionalNavigation="Contained" >
                                        <StackPanel x:Uid="StackPanel_1" IsItemsHost="True"  />
                                    </ScrollViewer>
                                </Border>
                            </Grid>
                        </Popup>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Uid="VisualStateGroup_1" x:Name="CommonStates">
                                <VisualState x:Uid="VisualState_1" x:Name="Normal" />
                                <VisualState x:Uid="VisualState_2" x:Name="MouseOver" />
                                <VisualState x:Uid="VisualState_3" x:Name="Disabled"/>
                            </VisualStateGroup>
                            <VisualStateGroup x:Uid="EditStates" x:Name="EditStates">
                                <VisualState x:Uid="Editable" x:Name="Editable">
                                    <Storyboard x:Uid="Storyboard_2">
                                         <ObjectAnimationUsingKeyFrames x:Uid="ObjectAnimationUsingKeyFrames_3" Storyboard.TargetProperty="(UIElement.Visibility)"
                                                   Storyboard.TargetName="PART_EditableTextBox">
                                            <DiscreteObjectKeyFrame x:Uid="DiscreteObjectKeyFrame_3" KeyTime="0" Value="{x:Static Visibility.Visible}" />
                                        </ObjectAnimationUsingKeyFrames>
                                      <ObjectAnimationUsingKeyFrames
                                             x:Uid="ObjectAnimationUsingKeyFrames_4" Storyboard.TargetProperty="(UIElement.Visibility)" Storyboard.TargetName="ContentSite">
                                            <DiscreteObjectKeyFrame x:Uid="DiscreteObjectKeyFrame_4" KeyTime="0" Value="{x:Static Visibility.Hidden}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Uid="Uneditable" x:Name="Uneditable" />
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger x:Uid="Trigger_2" Property="IsEnabled" Value="False">
                            <Setter x:Uid="Setter_11" TargetName="Grille" Property="Visibility" Value="Hidden"/>
                        </Trigger>
                     <!--   <Trigger Property="HasItems" Value="false">
                            <Setter TargetName="DropDownBorder" Property="MinHeight" Value="95" />
                        </Trigger>
                        <Trigger Property="IsGrouping" Value="true">
                            <Setter Property="ScrollViewer.CanContentScroll" Value="false" />
                        </Trigger>
                        <Trigger SourceName="Popup" Property="AllowsTransparency" Value="true">
                            <Setter TargetName="DropDownBorder" Property="CornerRadius" Value="4" />
                            <Setter TargetName="DropDownBorder" Property="Margin" Value="0,2,0,0" />
                        </Trigger>-->
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
    </Setter>
  </Style>


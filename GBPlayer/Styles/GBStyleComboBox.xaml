    <!--STYLE ITEM W8-->
    <Style x:Uid="Style_3" x:Key="ComboBoxItemW8" TargetType="{x:Type ComboBoxItem}">
        <Setter x:Uid="Setter_23" Property="SnapsToDevicePixels" Value="true" />
        <Setter x:Uid="Setter_24" Property="OverridesDefaultStyle" Value="true" />
        <Setter x:Uid="Setter_25" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_5" TargetType="{x:Type ComboBoxItem}">
                            <Border x:Uid="BorderItem" x:Name="BorderItem"
                            CornerRadius="3"
                            Margin="1"
                            SnapsToDevicePixels="true"
                            Background="Transparent"
                            BorderBrush="{StaticResource W8Border}"
                            BorderThickness="1">
                        <Label x:Uid="Conteneur" x:Name="Conteneur" VerticalAlignment="Center" 
                               Foreground="{StaticResource W8ForegroundWhite}" 
                                       FontFamily="{TemplateBinding FontFamily}" 
                                       FontSize="{TemplateBinding FontSize}" 
                                        Padding="1" HorizontalContentAlignment="Stretch"  >
                            <Border x:Uid="BorderItemContenu" x:Name="BorderItemContenu" Margin="1">
                                <ContentPresenter x:Uid="ContentPresenter_5" Height="16"/>
                            </Border>
                        </Label>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Uid="VisualStateGroup_6" x:Name="CommonStates">
                                <VisualState x:Uid="VisualState_15" x:Name="Normal" />
                                <VisualState x:Uid="VisualState_16" x:Name="MouseOver">
                                    <Storyboard x:Uid="Storyboard_5">
                                        <ObjectAnimationUsingKeyFrames x:Uid="ObjectAnimationUsingKeyFrames_9" Storyboard.TargetName="BorderItem"
                                                                           Storyboard.TargetProperty="Background">
                                            <ObjectAnimationUsingKeyFrames.KeyFrames>
                                                <DiscreteObjectKeyFrame x:Uid="DiscreteObjectKeyFrame_9" KeyTime="0" Value="{StaticResource W8BackgroundOver}"/>
                                            </ObjectAnimationUsingKeyFrames.KeyFrames>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Uid="Selected" x:Name="Selected">
                                    <Storyboard x:Uid="Storyboard_6">
                                        <ObjectAnimationUsingKeyFrames x:Uid="ObjectAnimationUsingKeyFrames_10" Storyboard.TargetName="BorderItem"
                                                                           Storyboard.TargetProperty="Background">
                                            <ObjectAnimationUsingKeyFrames.KeyFrames>
                                                <DiscreteObjectKeyFrame x:Uid="DiscreteObjectKeyFrame_10" KeyTime="0"  Value="{StaticResource W8BackgroundSelect}"/>
                                            </ObjectAnimationUsingKeyFrames.KeyFrames>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Uid="SelectionStates" x:Name="SelectionStates">
                                <VisualState x:Uid="Unselected" x:Name="Unselected" />
                                <VisualState x:Uid="SelectedUnfocused" x:Name="SelectedUnfocused">
                                    <Storyboard x:Uid="Storyboard_7">
                                        <ObjectAnimationUsingKeyFrames x:Uid="ObjectAnimationUsingKeyFrames_11" Storyboard.TargetName="BorderItem"
                                                                           Storyboard.TargetProperty="Background">
                                            <ObjectAnimationUsingKeyFrames.KeyFrames>
                                                <DiscreteObjectKeyFrame x:Uid="DiscreteObjectKeyFrame_11" KeyTime="0" Value="{StaticResource ItemSelectedInactiveFill}"/>
                                            </ObjectAnimationUsingKeyFrames.KeyFrames>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger x:Uid="Trigger_5" Property="IsMouseOver" Value="True">
                            <Setter x:Uid="Setter_26" TargetName="BorderItem" Property="BorderThickness" Value="1"/>
                            <Setter x:Uid="Setter_27" TargetName="BorderItem" Property="BorderBrush" Value="{StaticResource W8BorderGray}"/>
                        </Trigger>
                        <Trigger x:Uid="Trigger_6" Property="IsSelected" Value="True">
                            <Setter x:Uid="Setter_28" TargetName="BorderItem" Property="BorderThickness" Value="1"/>
                            <Setter x:Uid="Setter_29" TargetName="BorderItem" Property="BorderBrush" Value="{StaticResource W8BorderOver}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <!--STYLE ITEM AVEC DES BOUTONS AJOUT ET SUPPRESSION LORS DU FOCUS-->
    <Style x:Uid="Style_4" x:Key="ComboBoxItemAddDel" TargetType="{x:Type ComboBoxItem}">
        <Setter x:Uid="Setter_30" Property="SnapsToDevicePixels" Value="true" />
        <Setter x:Uid="Setter_31" Property="OverridesDefaultStyle" Value="true" />
        <Setter x:Uid="Setter_32" Property="Template">
            <Setter.Value>
                <ControlTemplate x:Uid="ControlTemplate_6" TargetType="{x:Type ComboBoxItem}">
